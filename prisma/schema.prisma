generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Waitlist {
  id        String   @id @default(cuid())
  email     String   @unique
  country   String
  createdAt DateTime @default(now())
}
